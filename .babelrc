{
	"presets": [
        "@babel/preset-env","@babel/preset-react"
    ],
	"plugins": [
		// "transform-decorators-legacy",
		"transform-regenerator",
		"syntax-async-functions"
	],
	"ignore": [
		"/node_modules"
	],
	"env": {
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs",
				["module-resolver", {
					"root": ["./src"]
				}]
			]
		}
	}
}